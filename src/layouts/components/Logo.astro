---
import { Image } from 'astro:assets';
import logo from '../../images/logoColor.svg';
// interface Props {}
---

<a href='/'>
  <Image
    src={logo}
    class='logo'
    alt='Logo de AstracodeLAB'
    loading='lazy'
    widths={[50, 90]}
    sizes='90px'
    sizes={`(max-width: 48rem) 3.125rem, (min-width: 64rem) 90px`}
  />
</a>
